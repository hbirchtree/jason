{
	"imports":
	[
		{
			"file": "native-common.json"
		}
	],
	"variables":
	[
		{
			"name": "STEAMAPPS",
			"value": "%HOME%/.local/share/wineprefixes/Steam/drive_c/Program Files/Steam/steamapps/common"
		},
		{
			"name": "WINEPREFIX",
			"value": "%WINEPREFIX_ROOT%/%WINEPREFIX_ID%"
		},
		{
			"name": "WINEPREFIX_ROOT",
			"value": "%HOME%/.local/share/wineprefixes"
		},
		{
			"name": "TOOLS",
			"value": "%HOME%/.local/share/winescripts/tools"
		},
		{
			"name": "WINEBUILDS",
			"value": "%HOME%/.local/share/wineversions"
		}
	],
	"subsystems":
	[
		{
			"type": "constant",
			"environment":
			[
				{
					"type": "variable",
					"name": "PULSE_LATENCY_MSEC",
					"value": "60"
				}
			]
		}
		
	],
	"wine.postrun":
	[
		{
			"title": "Setting gamma",
			"sys.exec": "xgamma -gamma 1.0"
		}
	],
	"systems":
	[
		{
			"type": "launcher",
			"identifier": "winery",
			"config-prefix": "wine",
			"inherit": "system",
			"launch-prefix": "wine",
			"variables":
			[
				{
					"type": "config-input",
					"name": "WINEVERSION",
					"input": "version"
				},
				{
					"type": "config-input",
					"name": "WINEPREFIX_ID",
					"input": "wineprefix-id"
				}
			],
			"environment":
			[
				{
					"type": "variable",
					"name": "PATH",
					"value": "%PATH%:%WINEBUILDS%/%WINEVERSION%/bin"
				},
				{
					"type": "variable",
					"name": "LD_LIBRARY_PATH",
					"value": "%WINEBUILDS%/%WINEVERSION%/lib:%WINEBUILDS%/%WINEVERSION%/lib64"
				},
				{
					"type": "variable",
					"name": "XDG_DATA_DIRS",
					"value": "%XDG_DATA_DIRS%:%WINEBUILDS%/%WINEVERSION%/share"
				},
				{
					"type": "variable",
					"name": "WINEPREFIX",
					"value": "%WINEPREFIX_ROOT%/%WINEPREFIX_ID%"
				}
			]
		},
		{
			"type": "launcher",
			"identifier": "system",
			"config-prefix": "sys",
			"launch-prefix": "",
			"environment":
			[
				{
					"type": "variable",
					"name": "IS_RUN_WITH_JASON",
					"value": "1"
				}
			]
		}
	]
}
